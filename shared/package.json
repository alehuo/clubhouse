{
  "name": "@alehuo/clubhouse-shared",
  "version": "1.2.8",
  "description": "Shared libraries for Clubhouse front-end and back-end.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "https://github.com/alehuo/clubhouse-shared",
  "author": "alehuo",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.1",
    "chai": "^4.2.0",
    "mocha": "^6.2.1",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "typescript": "3.6.4"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist/*.d.ts dist/*.js dist/*.d.ts.map dist/*.js.map",
    "test": "nyc mocha test/**/*.test.ts --exit --timeout 10000"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.ts",
      "dist/**/*.js",
      "coverage/**/*.js",
      "src/index.ts",
      "src/Permissions.ts"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "perFile": true,
    "all": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  }
}
